{
  "name": "@harmoni/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next",
    "db:generate": "node -r dotenv/config -e \"require('dotenv').config(); require('child_process').execSync('npx prisma generate', {stdio: 'inherit'})\"",
    "db:push": "node -r dotenv/config -e \"require('dotenv').config(); require('child_process').execSync('npx prisma db push', {stdio: 'inherit'})\"",
    "db:migrate": "node -r dotenv/config -e \"require('dotenv').config(); require('child_process').execSync('npx prisma migrate dev', {stdio: 'inherit'})\"",
    "db:seed": "node -r dotenv/config -e \"require('dotenv').config(); require('child_process').execSync('npx tsx prisma/seed.ts', {stdio: 'inherit'})\"",
    "db:studio": "node -r dotenv/config -e \"require('dotenv').config(); require('child_process').execSync('npx prisma studio', {stdio: 'inherit'})\""
  },
  "dependencies": {
    "@harmoni/ui": "*",
    "@neondatabase/serverless": "^0.9.0",
    "@prisma/client": "^5.9.0",
    "@tanstack/react-query": "^5.17.0",
    "@tanstack/react-query-devtools": "^5.91.3",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.6",
    "dotenv": "^16.4.0",
    "lucide-react": "^0.309.0",
    "next": "^14.1.0",
    "next-link": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.3",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7",
    "@google/genai": "^1.0.0",
    "pdf-lib": "^1.17.1",
    "framer-motion": "^10.16.4",
    "react-speech-recognition": "^3.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8.4.33",
    "prisma": "^5.9.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
